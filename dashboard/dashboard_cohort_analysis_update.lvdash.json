      "queryLines": [
        "SELECT * FROM workspace.bigquery_db_cohort_db.gold_repeat_purchase_rate_by_cohort"
      ]
    },
    {
      "name": "2e907158",
      "displayName": "gold_retention_rate_by_cohort",
      "queryLines": [
        "SELECT * FROM workspace.bigquery_db_cohort_db.gold_retention_rate_by_cohort"
      ]
    },
    {
      "name": "5f65f36e",
      "displayName": "Average 1-Month Retention",
      "queryLines": [
        "SELECT ROUND(AVG(retention_rate_1m)) AS avg_1m_retention\n",
        "FROM gold_retention_rate_by_cohort;\n"
      ],
      "catalog": "workspace",
      "schema": "bigquery_db_cohort_db"
    },
    {
      "name": "c0b192c7",
      "displayName": "Total Customers",
      "queryLines": [
        "SELECT SUM(num_customers) AS total_new_customers\n",
        "FROM gold_cohort_size_by_month;"
      ],
      "catalog": "workspace",
      "schema": "bigquery_db_cohort_db"
    },
    {
      "name": "f18f50c4",
      "displayName": "Acquisition Drop (%)",
      "queryLines": [
        "SELECT \n",
        "  ROUND((1 - \n",
        "    ( (SELECT num_customers FROM gold_cohort_size_by_month ORDER BY cohort_month DESC LIMIT 1)\n",
        "      /\n",
        "      (SELECT num_customers FROM gold_cohort_size_by_month ORDER BY cohort_month ASC LIMIT 1)\n",
        "    )\n",
        "  ) * 100) AS acquisition_drop_pct;\n"
      ],
      "catalog": "workspace",
      "schema": "bigquery_db_cohort_db"
    },
    {
      "name": "e48cc0c3",
      "displayName": "Avg Repeat Rate (2+ Orders)",
      "queryLines": [
        "SELECT ROUND(AVG(pct_2plus_orders)) AS avg_repeat_rate\n",
        "FROM gold_repeat_purchase_rate_by_cohort;"
      ],
      "catalog": "workspace",
      "schema": "bigquery_db_cohort_db"
    },
    {
      "name": "cc7d773f",
      "displayName": "Heatmap-Query",
      "queryLines": [
        "SELECT\n",
        "  cohort_month,\n",
        "  'month0' AS period,\n",
        "  100 AS retention\n",
        "FROM gold_retention_rate_by_cohort\n",
        "\n",
        "UNION ALL\n",
        "SELECT\n",
        "  cohort_month,\n",
        "  'month1' AS period,\n",
        "  retention_rate_1m AS retention\n",
        "FROM gold_retention_rate_by_cohort\n",
        "\n",
        "UNION ALL\n",
        "SELECT\n",
        "  cohort_month,\n",
        "  'month2' AS period,\n",
        "  retention_rate_2m AS retention\n",
        "FROM gold_retention_rate_by_cohort\n",
        "\n",
        "UNION ALL\n",
        "SELECT\n",
        "  cohort_month,\n",
        "  'month3' AS period,\n",
        "  retention_rate_3m AS retention\n",
        "FROM gold_retention_rate_by_cohort\n",
        "ORDER BY cohort_month, period;\n"
      ],
      "catalog": "workspace",
      "schema": "bigquery_db_cohort_db"
    },
    {
      "name": "02b1323a",
      "displayName": "Time to 2nd Purchase",
      "queryLines": [
        "SELECT \n",
        "  DATE_FORMAT(DATE_TRUNC('month', first_purchase_date), 'yyyy-MM') AS cohort_month,\n",
        "  ROUND(AVG(days_between_first_and_second)) AS avg_days_to_repurchase,\n",
        "  ROUND(PERCENTILE(days_between_first_and_second, 0.5)) AS median_days_to_repurchase\n",
        "FROM workspace.bigquery_db_cohort_db.silver_cohort_analysis\n",
        "WHERE second_purchase_date IS NOT NULL\n",
        "GROUP BY cohort_month\n",
        "ORDER BY cohort_month"
      ],
      "catalog": "workspace",
      "schema": "bigquery_db_cohort_db"
    },
    {
      "name": "e26ffa0c",
      "displayName": "AVG days to second purchase",
      "queryLines": [
        "SELECT \n",
        "  ROUND(AVG(days_between_first_and_second)) AS avg_days_to_repurchase\n",
        "FROM workspace.bigquery_db_cohort_db.silver_cohort_analysis\n",
        "WHERE second_purchase_date IS NOT NULL"
      ],
      "catalog": "workspace",
      "schema": "bigquery_db_cohort_db"
    }
  ],
